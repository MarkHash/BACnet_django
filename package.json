{
  "name": "bacnet_django",
  "version": "1.0.0",
  "description": "A Django web application for discovering, monitoring, and reading BACnet devices on your network. This application provides a user-friendly web interface for BACnet device discovery and real-time sensor data monitoring.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
      "jest": "^30.1.3",
      "jest-environment-jsdom": "^29.0.0",
      "@testing-library/jest-dom": "^6.8.0",
      "jsdom": "^26.1.0"
    },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/discovery/static/js/tests/setup.js"],
    "testMatch": ["<rootDir>/discovery/static/js/tests/**/*.test.js"],
    "collectCoverageFrom": [
      "discovery/static/js/**/*.js",
      "!discovery/static/js/tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkHash/BACnet_django.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarkHash/BACnet_django/issues"
  },
  "homepage": "https://github.com/MarkHash/BACnet_django#readme"
}
